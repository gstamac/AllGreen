<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="AllGreen.Runner.WPF.MainWindow"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        xmlns:agrc="clr-namespace:AllGreen.Runner.WPF.Core;assembly=AllGreen.Runner.WPF.Core"
        Title="AllGreen" Height="500" Width="1300">
    <i:Interaction.Triggers>
        <i:EventTrigger EventName="Loaded">
            <i:InvokeCommandAction Command="{Binding StartServerCommand}"/>
        </i:EventTrigger>
    </i:Interaction.Triggers>
    <agrc:MainView/>
</Window>
